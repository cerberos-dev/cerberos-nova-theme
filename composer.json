{
    "name": "cerberos/cerberos-theme",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 | ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Cerberos\\CerberosTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cerberos\\CerberosTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
