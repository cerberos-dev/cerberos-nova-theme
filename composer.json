{
    "name": "cerberos/cerberos-theme",
    "description": "A Laravel Nova theme.",
    "type": "library",
    "homepage": "https://github.com/cerberos-dev/cerberos-nova-theme",
    "authors": [
        {
            "name": "Cerberos",
            "homepage": "https://github.com/cerberos-dev",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/cerberos-dev/cerberos-nova-theme/issues",
        "source": "https://github.com/cerberos-dev/cerberos-nova-theme"
    },
    "keywords": [
        "laravel",
        "nova",
        "theme",
        "cerberos "
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0.2"
    },
    "autoload": {
        "psr-4": {
            "Cerberos\\CerberosTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cerberos\\CerberosTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
